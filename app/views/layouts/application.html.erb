<!DOCTYPE html>
<html>
  <head>
    <title>Template</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class='container'>
      <%= link_to 'Companies', companies_path, class: 'btn btn-primary' %>
      <%= link_to 'Employees', employees_path, class: 'btn btn-primary' %>
      <%= yield %>
    </div>
  </body>
</html>
